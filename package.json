    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "express": "^4.18.2",
    "jest": "29.5.0",
    "nestia": "^4.3.2",
    "prisma": "^4.15.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "src/(.*)$": "<rootDir>/../src/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
