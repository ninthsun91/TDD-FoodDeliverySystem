    "typia": "^4.1.0"
  },
  "devDependencies": {
    "@nestia/e2e": "^0.3.4",
    "@nestia/sdk": "^1.4.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.67",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
